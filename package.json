{
  "name": "ws-product-nodejs",
  "version": "0.1.0",
  "description": "API work samples",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node index.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' \" \"npm run client \"",
    "client": "cd client && npm run start",
    "pretest": "eslint --ignore-path .eslintignore .",
    "test": "jest",
    "build": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EQWorks/ws-product-nodejs.git"
  },
  "author": "woozyking",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EQWorks/ws-product-nodejs/issues"
  },
  "homepage": "https://github.com/EQWorks/ws-product-nodejs#readme",
  "dependencies": {
    "cookie-session": "^1.4.0",
    "express": "^4.15.3",
    "if-env": "^1.0.4",
    "jest": "^24.9.0",
    "pg": "^6.2.4"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "nodemon": "^1.11.0"
  }
}
